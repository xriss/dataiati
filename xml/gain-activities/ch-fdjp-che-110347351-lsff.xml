  <iati-identifier>CH-FDJP-CHE-110347351-LSFF</iati-identifier>
  <reporting-org ref="CH-FDJP-CHE-110347351" secondary-reporter="0" type="21">
   <narrative xml:lang="EN">The Global Alliance for Improved Nutrition</narrative>
