     <period-start iso-date="2021-01-01"></period-start>
     <period-end iso-date="2021-12-31"></period-end>
     <target>
      <comment>
       <narrative xml:lang="EN">2. 100% (2021)</narrative>
      </comment>
     </target>
     <actual>
      <comment>
       <narrative xml:lang="EN">2.  100% (2021)</narrative>
      </comment>
     </actual>
    </period>
    <period>
