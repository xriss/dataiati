  <result aggregation-status="1" type="1">
   <title>
    <narrative xml:lang="EN">Annual Output Results</narrative>
   </title>
   <indicator ascending="0" measure="1">
    <title>
     <narrative xml:lang="EN">Output Indicator</narrative>
    </title>
    <description>
     <narrative xml:lang="EN">Number of Inclusive, gender responsive, REDD+ policies and measures implemented by targeted REDD+ countries in LAC</narrative>
    </description>
    <baseline year="2017">
     <comment>
      <narrative xml:lang="EN">In 2017, only two UN-REDD partner countries are implementing inclusive, gender responsive policies and measures for REDD+ in Latin America (Ecuador and Costa Rica)</narrative>
     </comment>
    </baseline>
    <period>
     <period-start iso-date="2019-01-01"></period-start>
     <period-end iso-date="2019-12-31"></period-end>
     <target>
      <comment>
       <narrative xml:lang="EN">Four countries implementing inclusive, gender responsive REDD+ policies and measures in LAC</narrative>
      </comment>
     </target>
     <actual>
      <comment>
       <narrative xml:lang="EN">Four countries in Latin America were implementing inclusive, gender responsive REDD+ policies and measures: Ecuador, Chile, Colombia and Mexico.</narrative>
      </comment>
     </actual>
    </period>
    <period>
     <period-start iso-date="2021-01-01"></period-start>
     <period-end iso-date="2021-12-31"></period-end>
     <target>
      <comment>
       <narrative xml:lang="EN">Wrapping up activities; linking project results to NDC reviews and the Climate Promite initiative; operational closure by September 2021.</narrative>
      </comment>
     </target>
     <actual>
      <comment>
       <narrative xml:lang="EN">Project wrapped up and transitioned to the new UN-REDD strategic framework; results integrated into NDC reviews and the Climate Promite initiative; Operational closure by September 2021.</narrative>
      </comment>
     </actual>
    </period>
    <period>
     <period-start iso-date="2020-01-01"></period-start>
     <period-end iso-date="2020-12-31"></period-end>
     <target>
      <comment>
       <narrative xml:lang="EN">Five countries implementing inclusive, gender responsive REDD+ policies and measures in LAC</narrative>
      </comment>
     </target>
     <actual>
      <comment>
       <narrative xml:lang="EN">Seven countries in Latin America were implementing inclusive, gender responsive REDD+ policies and measures: Chile, Colombia, Costa Rica, Ecuador, Mexico, Paraguay, Peru.</narrative>
      </comment>
     </actual>
    </period>
