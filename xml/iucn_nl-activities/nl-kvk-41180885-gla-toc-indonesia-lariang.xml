  <iati-identifier>NL-KVK-41180885-GLA - ToC Indonesia Lariang</iati-identifier>
  <reporting-org ref="NL-KVK-41180885" type="21">
   <narrative xml:lang="EN">IUCN Netherlands Committee</narrative>
