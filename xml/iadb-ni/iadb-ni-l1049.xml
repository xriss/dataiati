 <iati-activity default-currency="USD" hierarchy="1" iati-activities:version="2.02" linked-data-uri="https://www.iadb.org/en/project/NI-L1049" xml:lang="EN" xmlns:iati-activities="http://d-portal.org/xmlns/iati-activities" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
