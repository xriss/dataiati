 <iati-activity default-currency="USD" hierarchy="1" iati-activities:version="2.02" linked-data-uri="https://www.iadb.org/en/project/PE-L1003" xml:lang="EN" xmlns:iati-activities="http://d-portal.org/xmlns/iati-activities">
