  <iati-identifier>US-GOV-7-TN-INSTITUTIONAL-CAPACITY-BUILDING-ICB</iati-identifier>
  <reporting-org ref="US-USAGOV" type="10">
   <narrative xml:lang="EN">The federal government of the United States</narrative>
