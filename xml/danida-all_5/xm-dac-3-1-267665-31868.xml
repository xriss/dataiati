  <result type="1">
   <title>
    <narrative xml:lang="EN">Five global roundtables</narrative>
   </title>
   <reference code="265688" vocabulary="99"></reference>
   <indicator aggregation-status="0" ascending="1" measure="1">
    <title>
     <narrative xml:lang="EN">Five global roundtables that each bring together 15-25 representatives from leading organisations representing the different stakeholder groups for a half day meeting.</narrative>
    </title>
    <period>
     <period-start iso-date="2018-01-01"></period-start>
