    <description>
     <narrative xml:lang="EN">Kedah State Protected Area Enactment.</narrative>
    </description>
    <baseline year="2020">
     <comment>
      <narrative xml:lang="EN">Kedah does not have its own Protected Area Enactment.</narrative>
     </comment>
    </baseline>
    <period>
     <period-start iso-date="2021-01-01"></period-start>
     <period-end iso-date="2021-12-31"></period-end>
     <target>
      <comment>
       <narrative xml:lang="EN">UNDP will engage with the Chief Ministers Office to arrange for a courtesy call and discussion on the draft of Kedah Protected Area Enactment.</narrative>
      </comment>
     </target>
     <actual>
      <comment>
       <narrative xml:lang="EN">The engagement with the Chief Ministers Office concerning the draft of Kedah Protected Area Enactment was postponed to 2022.</narrative>
      </comment>
     </actual>
    </period>
    <period>
     <period-start iso-date="2020-01-01"></period-start>
     <period-end iso-date="2020-12-31"></period-end>
     <target>
      <comment>
       <narrative xml:lang="EN">Formulation and/or drafting of Kedah State Protected Area Enactment.</narrative>
      </comment>
     </target>
     <actual>
      <comment>
       <narrative xml:lang="EN">The final draft of Kedah State Protected Area Enactment was sent to Director of Kedahs Economic Planning Unit on 16 July 2020.</narrative>
      </comment>
     </actual>
    </period>
