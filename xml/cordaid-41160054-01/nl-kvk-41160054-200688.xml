  <description type="1">
   <narrative xml:lang="EN">Providing emergency primary health care services and psycho-social support for newly displaced Syrian refugees arriving from Northeast Syria in Gawilan camp</narrative>
  </description>
  <description type="3">
   <narrative xml:lang="EN">Syrian refugees</narrative>
  </description>
