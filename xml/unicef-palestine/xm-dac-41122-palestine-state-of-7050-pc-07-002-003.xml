  <conditions attached="1">
   <condition type="1">
    <narrative xml:lang="EN">Should contribute directly to results for children as outlined in the UNDAF, national results and/or UNICEF Strategic Plan and within the scope defined in the CPD, CPAP, OMP or other agreed upon programme documents</narrative>
   </condition>
