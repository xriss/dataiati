  <title xml:lang="EN">
   <narrative xml:lang="EN">Support to Poverty Reduction Strategy Vietnam 2009-2012</narrative>
  </title>
  <description type="1" xml:lang="EN">
   <narrative xml:lang="EN">N/A</narrative>
  </description>
  <description type="3" xml:lang="EN">
