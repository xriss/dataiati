  <title>
   <narrative xml:lang="EN">Libya - Flash Appeal 2011 Islamic Relief Sweden - Libya - humanitarian Flash App</narrative>
   <narrative xml:lang="SV">Libyen - blixtappell 2011 Islamic Relief Sverige</narrative>
