  <title>
   <narrative xml:lang="EN">Citizen-lead community development</narrative>
  </title>
  <description type="1">
   <narrative xml:lang="EN">LDA collects and disseminates best practices from Laos and Southeast Asia through an online Knowledge and Information System and three workshops at national level.</narrative>
  </description>
  <participating-org ref="XI-IATI-EC_DEVCO" role="1" type="10">
   <narrative xml:lang="EN">European Union</narrative>
