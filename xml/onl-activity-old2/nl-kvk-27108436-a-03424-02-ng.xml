  <title>
   <narrative xml:lang="EN">Strengthening Humanitarian Response</narrative>
  </title>
  <description type="1">
   <narrative xml:lang="EN">The project seeks to reduce humanitarian crisis arising from disaster through building multilevel and multi-sectorial collaborativestructure for proactive and effective disaster mitigation, preparedness and humanitarian response in Nigeria</narrative>
  </description>
  <participating-org ref="NL-KVK-27108436" role="1" type="21">
