   <reference code="263995" vocabulary="99"></reference>
   <indicator aggregation-status="0" ascending="0" measure="1">
    <title>
     <narrative xml:lang="EN">Styrkelse af kommuners kapaciteter inden for lokal ledelse, governance og lokalt demokrati samt decentralisering.</narrative>
    </title>
    <period>
     <period-start iso-date="2016-01-01"></period-start>
     <period-end iso-date="2016-12-31"></period-end>
