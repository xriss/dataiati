 <iati-activity default-currency="USD" hierarchy="3" iati-activities:linked-data-default="https://data.mcc.gov/" iati-activities:version="2.01" xml:lang="EN" xmlns:iati-activities="http://d-portal.org/xmlns/iati-activities" xmlns:mcc="https://data.mcc.gov/ns#" xmlns:usg="http://foreignassistance.gov/iati#">
