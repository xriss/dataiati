  <iati-identifier>GB-CHC-282305-Networks_for_Peace_Africa</iati-identifier>
  <reporting-org ref="GB-CHC-282305" secondary-reporter="1" type="21">
   <narrative xml:lang="EN">Minority Rights Group</narrative>
