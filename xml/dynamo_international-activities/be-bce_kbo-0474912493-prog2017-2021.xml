  <iati-identifier>BE-BCE_KBO-0474912493-PROG2017-2021</iati-identifier>
  <reporting-org ref="BE-BCE_KBO-0474912493" type="22">
   <narrative xml:lang="FR">Dynamo International asbl</narrative>
