  <activity-status code="2"></activity-status>
  <activity-date iso-date="2019-10-01" type="1">
   <narrative xml:lang="EN">The project for the benefit of over 6000 women and girls will run from 1 October 2019 to 30 September 2022 and will be implemented in the Ugandan districts of Tororo , Budaka and Bukedea</narrative>
  </activity-date>
  <recipient-country code="UG" percentage="100">
   <narrative xml:lang="EN">The project is for implementation across the districts of Tororo, Budaka and Bukedea in eastern Uganda</narrative>
  </recipient-country>
  <sector code="15180" percentage="100" vocabulary="1">
   <narrative xml:lang="EN">The project aims to have a positive impact on beliefs, behaviours, practices and attitudes related to violence against women and girls</narrative>
  </sector>
