  <iati-identifier>US-EIN-11-3803281-Analysis- MaterCard Grant</iati-identifier>
  <reporting-org ref="US-EIN-11-3803281" type="80">
   <narrative xml:lang="EN">African Center for Economic Transformation</narrative>
