  <result aggregation-status="1" type="1">
   <title>
    <narrative xml:lang="EN">HC.R1 Improved capacity of local and national authorities and local humanitarian NGO&apos;s to respond to the humanitarian needs in North East Nigeria</narrative>
   </title>
   <indicator measure="1">
    <title>
     <narrative xml:lang="EN"># of authorities who have contributed in the development of the Emergency Preparedness Response Plan</narrative>
    </title>
    <description>
     <narrative xml:lang="EN">Develop an Emergency Preparedness Response Plan with local authorities, including State Emergency Management Agency (SEMA), and carry out joint emergency response activities to support hands on training and mentoring in basic humanitarian principles and response</narrative>
    </description>
    <reference code="HC.1I1" vocabulary="99"></reference>
    <period>
     <period-start iso-date="2015-05-16"></period-start>
     <period-end iso-date="2016-05-15"></period-end>
     <target value="60"></target>
     <actual value="189"></actual>
    </period>
   </indicator>
  </result>
  <result type="1">
