    <description>
     <narrative xml:lang="EN"># of ventilators and PPE procured in government hospitals across target states</narrative>
    </description>
    <baseline year="2020">
     <comment>
      <narrative xml:lang="EN">0</narrative>
     </comment>
    </baseline>
    <period>
     <period-start iso-date="2020-01-01"></period-start>
