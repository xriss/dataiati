 <iati-activity default-currency="GBP" hierarchy="0" humanitarian="1" iati-activities:version="2.02" linked-data-uri="http://www.plan-uk.org/iati" xml:lang="EN" xmlns:iati-activities="http://d-portal.org/xmlns/iati-activities" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
